      <a href="#" onclick="new Ajax.Updater('preview_mono_250', '/monologues/preview?monologue_id=250', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('lcTR5/tmwCtOJRqrgln3NrKMvPBmTy6PZRreJjCwnZs=')}); return false;">hide</a>
