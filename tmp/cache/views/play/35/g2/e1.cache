      <a href="#" onclick="new Ajax.Updater('preview_mono_253', '/monologues/preview?monologue_id=253', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('lcTR5/tmwCtOJRqrgln3NrKMvPBmTy6PZRreJjCwnZs=')}); return false;">hide</a>
