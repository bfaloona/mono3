      <a href="#" onclick="new Ajax.Updater('preview_mono_253', '/monologues/preview?monologue_id=253', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('LFi8KCV3jqu0kabygn/HeskmEqQNjlBI8iUvmtC9RUE=')}); return false;">hide</a>
