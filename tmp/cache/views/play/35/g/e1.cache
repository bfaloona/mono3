      <a href="#" onclick="new Ajax.Updater('preview_mono_265', '/monologues/preview?monologue_id=265', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('LFi8KCV3jqu0kabygn/HeskmEqQNjlBI8iUvmtC9RUE=')}); return false;">hide</a>
