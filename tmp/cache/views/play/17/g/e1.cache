      <a href="#" onclick="new Ajax.Updater('preview_mono_839', '/monologues/preview?monologue_id=839', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('VAo/o3//2cVPe1Ff1DpHtRdxzOZYgHqXt9ohe+nhL5Y=')}); return false;">hide</a>
