      <a href="#" onclick="new Ajax.Updater('preview_mono_282', '/monologues/preview?monologue_id=282', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('7lw6GjKO7weQB8EaAHGydwG1ILdnNDkMvy/aCa0HgBs=')}); return false;">hide</a>
      <br/>
