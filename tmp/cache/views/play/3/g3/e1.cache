      <a href="#" onclick="new Ajax.Updater('preview_mono_281', '/monologues/preview?monologue_id=281', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('7lw6GjKO7weQB8EaAHGydwG1ILdnNDkMvy/aCa0HgBs=')}); return false;">hide</a>
