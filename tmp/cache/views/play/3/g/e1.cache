      <a href="#" onclick="new Ajax.Updater('preview_mono_117', '/monologues/preview?monologue_id=117', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('T8TIqT7hfryND7YwKO9JQZ3uLdlAHKCSp8rqDTvr1Hc=')}); return false;">hide</a>
