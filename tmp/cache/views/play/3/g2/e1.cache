      <a href="#" onclick="new Ajax.Updater('preview_mono_116', '/monologues/preview?monologue_id=116', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('S69vVMNMQMOWIZenOBqlhwlMXF96A7cQw5+3M4BLnGg=')}); return false;">hide</a>
