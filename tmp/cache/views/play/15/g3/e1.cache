      <a href="#" onclick="new Ajax.Updater('preview_mono_388', '/monologues/preview?monologue_id=388', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('8Shgl5uPiYRHOdTj1/QACLmWwEIEi9OgTlSzozv7ZSk=')}); return false;">hide</a>
