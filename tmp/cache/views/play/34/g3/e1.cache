      <a href="#" onclick="new Ajax.Updater('preview_mono_656', '/monologues/preview?monologue_id=656', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('E+HCRGMqWBU6t1r6BcA/QtRQCrt7/XhbRaaRTQDFe3I=')}); return false;">hide</a>
      <br/>
