      <a href="#" onclick="new Ajax.Updater('preview_mono_137', '/monologues/preview?monologue_id=137', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('BqqLRE7K+vstSmG2ZEtlX+P6mc7pihrhKQfPTXsVVXQ=')}); return false;">hide</a>
      <br/>
