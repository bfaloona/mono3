      <a href="#" onclick="new Ajax.Updater('preview_mono_145', '/monologues/preview?monologue_id=145', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('ADIfMYdbz1Ay4DxlDoLLm+f+wGBj9m9N2QrfbTveNSc=')}); return false;">hide</a>
