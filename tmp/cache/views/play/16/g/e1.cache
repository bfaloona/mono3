      <a href="#" onclick="new Ajax.Updater('preview_mono_409', '/monologues/preview?monologue_id=409', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('AO3uD52WAElbpWyBwEwH5PWk0uOIjn+dMrvCvCJkzYQ=')}); return false;">hide</a>
