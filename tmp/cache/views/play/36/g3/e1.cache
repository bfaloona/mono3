      <a href="#" onclick="new Ajax.Updater('preview_mono_685', '/monologues/preview?monologue_id=685', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('pQK/rTq9qP0pBtdtAd9YO9xLCo9bKFSlL6rDOTkp4uM=')}); return false;">hide</a>
