      <a href="#" onclick="new Ajax.Updater('preview_mono_10', '/monologues/preview?monologue_id=10', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('y+ttzZQbrZXrUkxAFxUWk5RVk6vaM7oAt9UoDqoM4iY=')}); return false;">hide</a>
