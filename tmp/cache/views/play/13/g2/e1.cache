      <a href="#" onclick="new Ajax.Updater('preview_mono_36', '/monologues/preview?monologue_id=36', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('aUL1YOZSq7ELA9rPA53MkwrHic7Xrv2CNcUt674qc7M=')}); return false;">hide</a>
