      <a href="#" onclick="new Ajax.Updater('preview_mono_34', '/monologues/preview?monologue_id=34', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('EjRYD88dqE8Vwq5XebPWCmgDFSMS80BCvnhtZmOZyas=')}); return false;">hide</a>
