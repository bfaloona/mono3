      <a href="#" onclick="new Ajax.Updater('preview_mono_38', '/monologues/preview?monologue_id=38', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('EjRYD88dqE8Vwq5XebPWCmgDFSMS80BCvnhtZmOZyas=')}); return false;">hide</a>
