      <a href="#" onclick="new Ajax.Updater('preview_mono_35', '/monologues/preview?monologue_id=35', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('EjRYD88dqE8Vwq5XebPWCmgDFSMS80BCvnhtZmOZyas=')}); return false;">hide</a>
