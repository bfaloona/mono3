      <a href="#" onclick="new Ajax.Updater('preview_mono_55', '/monologues/preview?monologue_id=55', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('BYkfOIElftK21sR0sfq7hrI+CT4AahcaYZ9fGxtDeMY=')}); return false;">hide</a>
