      <a href="#" onclick="new Ajax.Updater('preview_mono_355', '/monologues/preview?monologue_id=355', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('8e+t74MWzqueMPFq5ray8DrdHYj0qFEGFmbQRbrSwGM=')}); return false;">hide</a>
