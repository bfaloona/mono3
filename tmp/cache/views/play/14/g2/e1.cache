      <a href="#" onclick="new Ajax.Updater('preview_mono_749', '/monologues/preview?monologue_id=749', {asynchronous:true, evalScripts:true, parameters:'authenticity_token=' + encodeURIComponent('3x+0J47yroH+pGOJLWeTXsrOFdvyHv54nijhY82JD08=')}); return false;">hide</a>
      <br/>
